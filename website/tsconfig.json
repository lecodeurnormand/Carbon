{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"],
      "@/common/*": ["/common/*"],
      "@/services/*": ["/services/*"],
      "@/components/*": ["/components/*"],
      "@/assets/*": ["/assets/*"],
      "@/styles/*": ["/src/styles/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Form/index.jsx", "src/pages/index.jsx", "src/pages/_document.jsx", "src/pages/_app.jsx"],
  "exclude": ["node_modules"]
}
